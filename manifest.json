{
  "name": "Give me the F-ing PDF",
  "author": "Vince Buffalo",
  "description": "Changes links to PDFs of journal articles to the PDF version (not HTML+PDF version with frames)",

  "manifest_version": 2,
  "version": "1.2",
  "permissions": [
    "http://*/*"
    ],
  "content_scripts": [
  {
    "matches": ["http://*/*"],
    "js": ["jquery-2.1.1.min.js", "gmtfpdf.js"],
    "run_at": "document_end"
  },
  { "matches": ["http://www.jstor.org/*/*"],
    "js": ["jquery-2.1.1.min.js", "jstor.js"],
    "run_at": "document_end"
  }
  ]
}
